							{
								"source": {
									"path": "$['Password']"
								},
								"sink": {
									"name": "Password",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "Admin",
						"type": "DatasetReference"
